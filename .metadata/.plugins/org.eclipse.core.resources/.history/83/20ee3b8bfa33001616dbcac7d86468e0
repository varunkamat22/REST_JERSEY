package printer;

import javax.ws.rs.FormParam;
import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement
public class Printer {
	@FormParam("id")
	private String id;
	@FormParam("location")
	private String location;
	
	public String getId() {
		return id;
	}
	public String getLocation() {
		return location;
	}
	
	
}
